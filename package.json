    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-hb": "^8.0.0",
    "gulp-json-editor": "^2.5.6",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-zip": "^5.1.0",
    "handlebars": "^4.7.7",
    "helper-markdown": "^1.0.0",
