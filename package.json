{
  "private": true,
  "name": "wp-plugin-template",
  "title": "WP Plugin Template",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "scripts": {
    "build": "NODE_ENV=production wp-scripts build assets/src/js/admin.js --output-path=assets/dist",
    "dev": "NODE_ENV=production wp-scripts start assets/src/js/admin.js --output-path=assets/dist",
    "format:js": "wp-scripts format-js assets/src",
    "lint:css": "wp-scripts lint-style assets/src",
    "lint:js": "wp-scripts lint-js assets/src",
    "packages-update": "wp-scripts packages-update",
    "release": "gulp release",
    "start": "wp-env start",
    "stop": "wp-env stop",
    "test": "gulp test",
    "wp-env": "wp-env"
  },
  "devDependencies": {
    "@wordpress/env": "^4.1.0",
